{
  "collection": "@nestjs/schematics",
  "sourceRoot": "server/src",
  "root": "server",
  "compilerOptions": {
    "tsConfigPath": "./tsconfig-server.build.json",
    "plugins": [
      {
        "name": "@nestjs/swagger",
        "options": {
          "dtoFileNameSuffix": [".dto.ts"]
        }
      }
    ]
  }
}
